{
	"name": "case-incremental-search",
	"displayName": "Case Incremental Search",
	"description": "Search for camel, snake ... cases",
	"version": "0.0.6",
	"publisher": "wsbak",
	"icon": "resources/search.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/wsbak/vscode-extension-case-incremental-search"
	},
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:caseSearch"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "case-incremental-search.start",
				"title": "Incremental search by case",
				"category": "Case Search"
			}
		],
		"keybindings": [
			{
				"command": "case-incremental-search.start",
				"key": "ctrl+f1",
				"mac": "cmd+f1"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"copy-to-media": "copyfiles --flat out/mmi_media.js media/",
		"compile-to-media": "npm run compile-tests && npm run copy-to-media",
		"compile": "webpack && npm run copy-to-media",
		"watch": "npm run compile-to-media && webpack --watch",
		"package": "npm run compile-to-media && webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test",
		"ui-test": "npm run compile-tests && extest setup-and-run './out/ui-test/**/*.test.js' -u -i -r . -e ./test-extensions"
	},
	"devDependencies": {
		"@types/chai": "^4.3.14",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.88.0",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"@vscode/test-cli": "^0.0.8",
		"@vscode/test-electron": "^2.3.9",
		"chai": "^4.4.1",
		"copyfiles": "^2.4.1",
		"eslint": "^8.57.0",
		"fast-glob": "^3.3.2",
		"mocha": "^10.0.6",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"vscode-extension-tester": "^8.5.0",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"change-case": "^4.1.2"
	}
}
